<div class="container-fluid">
		<div class="row1">
			
		
				<h1> Results </h1>

	<h2> You selected <%= @activity_name %> in <%= @area_name %> <button class=" button-result btn"><h4>
          <a class="sup" href="/">
    
    Not right? Go Back</a>
  </button></h4></h2>
	

</div>

<div class="row2">
	<div class="col-sm-4">
<ul class="today-class"> 
	<h2> Classes Available Today </h2>
	<div class ="scrollable-menu">
		<% @activity_today.each do |today|%>
			<li class="class-time scrollable-menu">
				<%= today.name%><br>
				<%= today.activity%><br>
				<%= today.time%><br>
				<img src="/assets/location.png" height="20" width="20"><a href="http://maps.google.com/maps?q=<%= today.lat %> ,<%= today.lng %>"><%= today.address%></a><br>

				<img src="/assets/phone.png" height="20" width="20"><%= today.phone%><br>	
				
		
		<%= today.description%><br>
		
		
	

			</li>
		<% end %>
	</div>
	</ul>
	</div>
	<div class="col-sm-4">

<ul class="tomorrow-class scrollable-menu">
	<h2> Tomorrow </h2>
	<div class ="scrollable-menu">
	<% @activity_tmr.each do |tmr|%>
			<li class="class-time scrollable-menu">
				<%= tmr.activity%><br>
		<%= tmr.time %>
		

			</li>
		<% end %>
	</div>
	</ul>
</div>
<div class="col-sm-4">
	<ul class="others scrollable-menu">
		<h2>Later this week</h2>
		<div class ="scrollable-menu">
	<% @act.each do |show| %>
	<li class="class-time scrollable-menu">
		<%= show.name %><br>
		<%= show.address %><br>
		<%= show.phone %><br>
		<%= show.activity %><br>
		<%= show.area %><br>
		<%= show.time %><br>
		<%= show.description %>


	</li>
<% end %>
</div>
</ul>

